{
  "name": "piche.ui",
  "description": "piche.ui",
  "author": "Arif Mammadov",
  "license": "CC-BY-NC",
  "version": "1.1.461",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./datetime": {
      "import": "./dist/datetime.js",
      "require": "./dist/datetime.umd.cjs"
    },
    "./file-viewer": {
      "import": "./dist/file-viewer.js",
      "require": "./dist/file-viewer.umd.cjs"
    },
    "./charts": {
      "import": "./dist/charts.js",
      "require": "./dist/charts.umd.cjs"
    },
    "./date-picker": {
      "import": "./dist/date-picker.js",
      "require": "./dist/date-picker.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.2.5",
    "@mui/material": "^5.16.7",
    "@mui/x-charts": "^8.5.2",
    "@mui/x-date-pickers": "^7.5.1",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.11",
    "dayjs-plugin-utc": "^0.1.2",
    "docx-preview": "^0.3.5",
    "mui-tel-input": "^5.1.2",
    "papaparse": "^5.5.2",
    "piche.npm.core": "^1.1.106",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.2.1"
  },
  "devDependencies": {
    "@fontsource/poppins": "^5.0.8",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-themes": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/node": "^20.2.5",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "typescript": "^5.0.4",
    "vite": "^4.5.3",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-html-env": "^1.2.8"
  },
  "lint-staged": {
    "*.{ts,tsx,json,html,scss}": [
      "pnpm prettier:check"
    ],
    "*.{ts,tsx}": [
      "pnpm lint",
      "bash -c 'pnpm types'"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings=0 --cache --color",
    "lint:fix": "eslint . --cache --fix --ext ts,tsx --report-unused-disable-directives --max-warnings=0",
    "format": "prettier --write .",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,json,scss,html}\"",
    "types": "tsc --pretty --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  }
}