{
  "name": "piche.npm.core",
  "description": "piche.npm.core",
  "author": "Kristaps Vitolins",
  "license": "CC-BY-NC",
  "version": "1.1.113",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "jsdom": "^22.1.0",
    "mobx": "^6.13.7",
    "nanoevents": "^9.1.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "typescript-optional": "3.0.0-alpha.3",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-html-env": "^1.2.8"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.12",
    "@types/node": "^20.2.5",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.64.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.4"
  },
  "lint-staged": {
    "*.{ts,tsx,json,html,scss}": [
      "pnpm prettier:check"
    ],
    "*.{ts,tsx}": [
      "pnpm lint",
      "bash -c 'pnpm types'"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings=0 --cache --color",
    "lint:fix": "eslint . --cache --fix --ext ts,tsx --report-unused-disable-directives --max-warnings=0",
    "format": "prettier --write .",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,json,scss,html}\"",
    "types": "tsc --pretty --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  }
}